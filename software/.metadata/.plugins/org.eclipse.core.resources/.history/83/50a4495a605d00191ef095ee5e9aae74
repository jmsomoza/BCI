void printTests(int * List, int list_size){

	int listOfIndexes[list_size];
	memset(listOfIndexes, 0, list_size*sizeof(int));

	int indx = peakFinder(List, list_size, listOfIndexes);

	for(int i = 0; i < indx; i++){
        printf("Index: %i Value: %i ListSize: %i\n", listOfIndexes[i], List[listOfIndexes[i]], list_size);
	}
	printf("\n");
}
